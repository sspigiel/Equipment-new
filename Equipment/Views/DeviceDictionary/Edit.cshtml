@model Equipment.Models.DeviceDictionary

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Edit</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>DeviceDictionary</h4>
        <hr />
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.DeviceDictionaryId)

        <div class="input-group">
            @Html.LabelFor(model => model.DeviceManufacturer, new { @class = "input-group-addon" })
            @Html.TextBoxFor(model => model.DeviceManufacturer, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.DeviceManufacturer)
        </div>

        <div class="input-group">
            @Html.LabelFor(model => model.DeviceName, new { @class = "input-group-addon" })
            @Html.TextBoxFor(model => model.DeviceName, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.DeviceName)
        </div>

        <div class="input-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
